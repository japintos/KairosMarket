{
  "name": "kairos-natural-market",
  "version": "1.0.0",
  "description": "Tienda online de productos naturales fraccionados - Kairos Natural Market",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "create-db": "cd server && npm run create-db",
    "migrate": "cd server && npm run migrate",
    "seed": "cd server && npm run seed",
    "setup": "npm run install-all && npm run create-db && npm run migrate && npm run seed",
    "start": "cd server && npm start",
    "test": "cd client && npm test",
    "lint": "cd client && npm run lint",
    "clean": "rm -rf node_modules && cd client && rm -rf node_modules && cd ../server && rm -rf node_modules"
  },
  "keywords": [
    "ecommerce",
    "productos-naturales",
    "react",
    "nodejs",
    "mysql",
    "mercadopago"
  ],
  "author": "Julio Alberto Pintos - WebXpert",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/kairos-natural-market.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/kairos-natural-market/issues"
  },
  "homepage": "https://github.com/tu-usuario/kairos-natural-market#readme"
}
